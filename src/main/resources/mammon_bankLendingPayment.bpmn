  <process id="mammon_bankLendingPayment" name="银行流贷/非金融机构还款审批流程" isExecutable="true">
    <startEvent id="startevent1" name="Start"></startEvent>
    <userTask id="highFundPersonTask" name="高级资金专员" activiti:candidateGroups="${mammon_group_high_fund_person}"></userTask>
    <userTask id="veepTask" name="副总裁" activiti:candidateGroups="${mammon_group_veep}"></userTask>
    <userTask id="cooTask" name="COO" activiti:candidateGroups="${mammon_group_coo}"></userTask>
    <sequenceFlow id="flow4" sourceRef="veepTask" targetRef="exclusivegateway3"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow5" name="通过" sourceRef="exclusivegateway2" targetRef="veepTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isApprove == 1}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway3" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow6" name="金额&gt;5万" sourceRef="exclusivegateway3" targetRef="cooTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isApprove == 1  && repayAmount > limitAmount}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent2" name="End"></endEvent>
    <sequenceFlow id="flow21" name="驳回" sourceRef="exclusivegateway3" targetRef="financePersonTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isApprove == 2}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow22" name="驳回" sourceRef="exclusivegateway2" targetRef="financePersonTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isApprove == 2}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="financePersonTask" name="申请人" activiti:candidateGroups="${mammon_group_finance_person}"></userTask>
    <exclusiveGateway id="exclusivegateway9" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow27" sourceRef="financePersonTask" targetRef="exclusivegateway9"></sequenceFlow>
    <sequenceFlow id="flow28" name="提交" sourceRef="exclusivegateway9" targetRef="highFundPersonTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isApprove == 1}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow32" sourceRef="startevent1" targetRef="financePersonTask"></sequenceFlow>
    <endEvent id="endevent3" name="End"></endEvent>
    <sequenceFlow id="flow33" name="作废" sourceRef="exclusivegateway9" targetRef="endevent3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isApprove == 2}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway10" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow35" sourceRef="cooTask" targetRef="exclusivegateway10"></sequenceFlow>
    <sequenceFlow id="flow36" sourceRef="exclusivegateway10" targetRef="endevent2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isApprove == 1}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow37" name="金额&lt;=5万" sourceRef="exclusivegateway3" targetRef="endevent2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isApprove == 1  && repayAmount <= limitAmount}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow38" name="驳回" sourceRef="exclusivegateway10" targetRef="financePersonTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isApprove == 2}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="usertask1" name="部门直属主管" activiti:candidateGroups="${mammon_group_control_director}"></userTask>
    <sequenceFlow id="flow41" sourceRef="usertask1" targetRef="exclusivegateway2"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway11" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow42" sourceRef="highFundPersonTask" targetRef="exclusivegateway11"></sequenceFlow>
    <sequenceFlow id="flow43" name="驳回" sourceRef="exclusivegateway11" targetRef="financePersonTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isApprove == 2}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow44" name="通过" sourceRef="exclusivegateway11" targetRef="usertask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isApprove == 1}]]></conditionExpression>
    </sequenceFlow>
